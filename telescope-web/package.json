{
  "name": "telescope-web",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --development",
    "build": "astro build",
    "cf": "astro build && wrangler dev",
    "db:setup": "node scripts/setup-local-db.js",
    "db:migrate": "wrangler d1 execute telescope-db --local --file=./db/schema.sql",
    "db:query": "wrangler d1 execute telescope-db --local --command",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "astro": "^5.16.11"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "adm-zip": "^0.5.16",
    "wrangler": "^4.59.2"
  }
}
